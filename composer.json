{
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=7.1.3",
        "algolia/algoliasearch-client-php": "^1.25",
        "atrox/haikunator": "^1.2",
        "doctrine/dbal": "^2.6",
        "fideloper/proxy": "~4.0",
        "laravel/framework": "5.6.*",
        "laravel/scout": "^4.0",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "~1.0",
        "league/commonmark": "^0.17.0",
        "torann/geoip": "^1.0"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.6.1",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "laravel-frontend-presets/tailwindcss": "^0.5.2",
        "laravel/dusk": "^3.0",
        "mockery/mockery": "~1.0",
        "nunomaduro/collision": "~2.0",
        "phpunit/phpunit": "~7.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": ["database/seeds", "database/factories"],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": ["laravel/dusk"]
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
